{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "author": "Janos Perge",
    "scripts": {
        "build": "vite build",
        "lint": "eslint resources/js --ext .js,.vue",
        "format": "prettier --write \"resources/js/**/*.{js,vue}\"",
        "format:check": "prettier --check \"resources/js/**/*.{js,vue}\"",
        "dev": "vite",
        "prepare": "husky setup",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-vue": "^6.0.3",
        "@vitest/coverage-v8": "^4.0.18",
        "@vue/test-utils": "^2.4.6",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-vue": "^10.6.2",
        "husky": "^8.0.0",
        "jsdom": "^28.1.0",
        "laravel-vite-plugin": "^2.0.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.7.4",
        "sass-embedded": "^1.97.1",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7",
        "vitest": "^4.0.18"
    },
    "dependencies": {
        "@primeuix/themes": "^2.0.2",
        "@primevue/forms": "^4.5.4",
        "chart.js": "^4.5.1",
        "pinia": "^3.0.4",
        "primeicons": "^7.0.0",
        "primevue": "^4.5.3",
        "vue-router": "^4.6.4"
    }
}
